import $assert;
import $core;

def $test_calls() {
  @core:ctrino.get_environment_variable("DURIAN_MAIN").then(fn $durian_main {
    def $p := new ($core:io:Process)();
    $p.executable := $durian_main;
    $p.start!();
    $p.exit_code.then(fn $exit_code {
      $assert:equals(71, $exit_code);
    });
  });
}

do {
  $test_calls();
}
